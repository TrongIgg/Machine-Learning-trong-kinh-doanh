tailsBtn = row.querySelector('.car-details-button');\n        return {\n          card: card,\n          model: modelEl ? modelEl.textContent.trim() : 'Vehicle',\n          price: priceEl ? priceEl.textContent.trim() : '',\n          detailsBtn: detailsBtn\n        };\n      }\n\n      function prefillContact(data) {\n        try {\n          var msg = document.getElementById('message');\n          if (msg) {\n            var base = 'Hi, I would like to request a quote for ' + data.model;\n            if (data.price) base += ' (' + data.price + ')';\n            base += '. Please share availability, total price, and any current offers.';\n            msg.value = base;\n          }\n          location.hash = '#contact';\n          var status = document.getElementById('hsb-aria-status');\n          if (status) status.textContent = 'Quote request started for ' + data.model + '.';\n        } catch (e) {}\n      }\n\n      function injectQuoteButtons() {\n        var rows = Array.from(document.querySelectorAll('.car-price-row'));\n        rows.forEach(function (row) {\n          if (row.querySelector('.rq-quote-btn')) return;\n          var info = getInfoFromRow(row);\n          if (!info) return;\n\n          if (info.detailsBtn) {\n            info.detailsBtn.style.marginLeft = 'auto';\n          }\n\n          var qBtn = createQuoteButton('Request a quote for ' + info.model);\n          qBtn.addEventListener('click', function () {\n            var original = qBtn.innerHTML;\n            qBtn.innerHTML = '<img src=\"https://api.iconify.design/lucide:check.svg?color=white&width=18&height=18\" alt=\"Started\">Quote Started';\n            qBtn.disabled = true;\n            prefillContact(info);\n            setTimeout(function () {\n              qBtn.innerHTML = original;\n              qBtn.disabled = false;\n            }, 1400);\n          });\n\n          row.appendChild(qBtn);\n        });\n      }\n\n      injectQuoteButtons();\n      try {\n        var mo = new MutationObserver(function (mutations) {\n          var needs = mutations.some(function (m) {\n            return Array.from(m.addedNodes || []).some(function (n) {\n              return n.nodeType === 1 && (n.matches('.car-card') || n.querySelector?.('.car-card'));\n            });\n          });\n          if (needs) injectQuoteButtons();\n        });\n        mo.observe(document.body, { childList: true, subtree: true });\n      } catch (e) {}\n\n      // 2) Hero search: filter cards (listings + featured)\n      var form = document.getElementById('hsb-search-form');\n      var input = document.getElementById('hsb-search-input');\n      var clearLink = document.getElementById('hsb-search-clear');\n      var summary = document.getElementById('hsb-search-summary');\n      var noResults = document.getElementById('hsb-no-results');\n\n      function allCards() {\n        var listing = Array.from(document.querySelectorAll('.car-card'));\n        var featured = Array.from(document.querySelectorAll('.featured-card'));\n        return listing.concat(featured);\n      }\n      function textForCard(card) {\n        var parts = [];\n        var model = card.querySelector('.car-model, .featured-model');\n        if (model) parts.push(model.textContent);\n        var desc = card.querySelector('.car-description');\n        if (desc) parts.push(desc.textContent);\n        var specs = card.querySelector('.featured-specs-row');\n        if (specs) parts.push(specs.textContent);\n        return parts.join(' ').toLowerCase();\n      }\n      function filterCards(query) {\n        var q = (query || '').trim().toLowerCase();\n        var cards = allCards();\n        var shown = 0;\n        if (!q) {\n          cards.forEach(function (c) { c.style.display = ''; });\n          shown = cards.length;\n        } else {\n          cards.forEach(function (c) {\n            var match = textForCard(c).includes(q);\n            c.style.display = match ? '' : 'none';\n            if (match) shown++;\n          });\n        }\n        if (summary) {\n          summary.textContent = q ? ('Showing ' + shown + ' result' + (shown === 1 ? '' : 's') + ' for \"' + query.trim() + '\"') : '';\n        }\n        if (noResults) {\n          noResults.style.display = q && shown === 0 ? 'block' : 'none';\n        }\n      }\n\n      form.addEventListener('submit', function (e) {\n        e.preventDefault();\n        filterCards(input.value);\n      });\n      input.addEventListener('input', function () {\n        filterCards(input.value);\n      });\n      clearLink.addEventListener('click', function (e) {\n        e.preventDefault();\n        input.value = '';\n        filterCards('');\n        input.focus();\n      });\n\n      filterCards('');\n    })();\n  "}]}]},{"tagName":"footer","name":"Site Footer","classes":["site-footer"],"components":[{"name":"Footer Inner","classes":["footer-inner"],"components":[{"name":"Footer Brand","classes":["footer-brand"],"components":[{"name":"Footer Brand Mark","classes":["footer-brand-mark"],"components":[{"tagName":"span","type":"text","name":"Footer Brand Initials","classes":["footer-brand-initials"],"components":[{"type":"textnode","content":"CS"}]}]},{"tagName":"span","type":"text","name":"Footer Brand Name","classes":["footer-brand-name"],"attributes":{"id":"i4a9ea"},"components":[{"type":"textnode","content":"LND Store"}]}]},{"name":"Footer Links","classes":["footer-links"],"components":[{"type":"link","name":"Footer Link Home","classes":["gjs-t-link","footer-link-home"],"attributes":{"href":"page://kichuziMUnLvoJphi"},"components":[{"type":"textnode","content":"Home"}]},{"type":"link","name":"Footer Link Cars","classes":["gjs-t-link","footer-link-cars"],"attributes":{"href":"page://BTR64n8f3lrY03Th"},"components":[{"type":"textnode","content":"Cars"}]},{"type":"link","name":"Footer Link Contact","classes":["gjs-t-link","footer-link-contact"],"attributes":{"href":"#contact"},"components":[{"type":"textnode","content":"Contact"}